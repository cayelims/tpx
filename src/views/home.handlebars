{{#each operator}}
	<div class="row justify-content-center">
		<div class="col-md">
			<div class="card box-shadow mb-5">
				<div class="card-body">
					<h5>{{dataValues.name}}</h5>
					<p class="card-text">{{dataValues.product_type}}</p>
					<a href="/operadoras/del/{{dataValues.id}}" class="text-danger">
						<i class="fas fa-trash"></i>
					</a>
				</div>
			</div>
		</div>
	</div>
{{/each}}